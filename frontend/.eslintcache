D);\n    const dispatch = useDispatch();\n\n    useEffect(() => {\n        // console.log(userId);\n        if (userId) {\n            console.log(\"User has successfully logged in!\");\n            dispatch(fetchUserPosts(userId))\n        }\n      \n    }, [])\n\n    return (\n        <div >\n            <div className=\"profileHeader\">\n                <div className=\"imageDiv\"></div>\n                <div className=\"avatarDiv\">\n                    <div >\n                        <Avatar id=\"pAvatar\" alt=\"John Doe\" src=\"\">J</Avatar>\n                    </div>\n                    <div>\n                        <h1>Fname Lname</h1>\n                    </div>\n                    <section className=\"profileOptions\">\n                        <div>\n                            <section>\n                                <h1>Posts</h1>\n                            </section>\n                            <section>\n                                <h1>About</h1>\n                            </section>\n                            <section>\n                                <h1>Friends</h1>\n                            </section>\n                            <section>\n                                <h1>Photos</h1>\n                            </section>\n                            <section>\n                                <h1>More</h1>\n                            </section>\n                        </div>\n                        <div>\n                            <section>\n                                <h5>Edit Profile</h5>\n                            </section>\n                            <section></section>\n                            <section></section>\n                            <section></section>\n                        </div>\n                    </section>\n                </div>\n            </div>\n            <div className=\"profileBody\">\n                <div className=\"leftSection\">\n                    <ProfileIntro />\n    