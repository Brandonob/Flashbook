 = useHistory();\n\n    const handleClick = async (e) => {\n        e.preventDefault();\n        logout();\n        dispatch(logOutUser())\n        history.push(\"/\")\n\n    }\n\n    useEffect(() => {\n        // console.log(userId);\n        if (userId) {\n            console.log(\"User has successfully logged in!\");\n            dispatch(fetchUserInfo(userId))\n            dispatch(fetchUserPosts(userId))\n          }\n         \n    }, [])\n\n    return (\n        <div className=\"home\">\n            <div className=\"leftSide\">\n                <LeftContainer/>\n            </div>\n            <div className=\"middle\">\n                <CreatePost/>\n                <Posts/>\n            </div>\n            <div className=\"rightSide\">\n                <h1>Contacts</h1>\n                <button onClick={handleClick}>Log Out</button>\n            </div>\n        </div>\n    )\n}\n\nexport default Home;\n","/Users/pursuit/Desktop/PersonalProjects/Flashbook/frontend/src/features/Posts/postsSlice.js",[],"/Users/pursuit/Desktop/PersonalProjects/Flashbook/frontend/src/features/Users/usersSlice.js",[],"/Users/pursuit/Desktop/PersonalProjects/Flashbook/frontend/src/features/Posts/Posts.js",["106"],"import React from 'react'\nimport { useSelector } from 'react-redux'\nimport { selectPosts } from './postsSlice'\nimport { selectInfo } from '../Users/usersSlice'\nimport Avatar from '@material-ui/core/Avatar'\nimport Likes from './Likes'\nimport './Post.css'\n\nconst Posts = () => {\n    const posts = useSelector(selectPosts)\n    const info = useSelector(selectInfo)\n    // debugger\n    \n    \n    return (\n        <div className=\"postContainer\">\n            {posts.map((post) => {\n                // debugger\n                return (\n                    <div className=\"post\" key={post.id}>\n                        <section className=\"infoSection\">\n                            <Avatar id=\"avatar\" alt=\"John Doe\" src={post.profile_pic} >J</Avatar>\n        