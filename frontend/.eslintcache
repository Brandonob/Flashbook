logout();\n        dispatch(logOutUser())\n        history.push(\"/\")\n\n    }\n\n    useEffect(() => {\n        // console.log(userId);\n        if (userId) {\n            console.log(\"User has successfully logged in!\");\n            dispatch(fetchUserInfo(userId))\n            dispatch(fetchUserPosts(userId))\n          }\n         \n    }, [])\n\n    return (\n        <div className=\"home\">\n            <div className=\"leftSide\">\n                <LeftContainer/>\n            </div>\n            <div className=\"middle\">\n                <CreatePost/>\n                <Posts/>\n            </div>\n            <div className=\"rightSide\">\n                <h1>Contacts</h1>\n                <button onClick={handleClick}>Log Out</button>\n            </div>\n        </div>\n    )\n}\n\nexport default Home;\n","/Users/pursuit/Desktop/PersonalProjects/Flashbook/frontend/src/features/Posts/postsSlice.js",[],"/Users/pursuit/Desktop/PersonalProjects/Flashbook/frontend/src/features/Users/usersSlice.js",[],"/Users/pursuit/Desktop/PersonalProjects/Flashbook/frontend/src/features/Posts/Posts.js",["103"],"/Users/pursuit/Desktop/PersonalProjects/Flashbook/frontend/src/features/Posts/CreatePost.js",["104","105","106"],"import React, {useState} from 'react'\nimport './CreatePost.css'\nimport Avatar from '@material-ui/core/Avatar'\nimport tagIcon from '../Web Icons/tagIcon.png'\nimport feelingIcon from '../Web Icons/feelingIcon.png'\nimport imageIcon from '../Web Icons/imageIcon.png'\nimport PostBuilder from './PostBuilder'\n\n\nconst CreatePost = ({userName}) => {\n    const [inputPlaceholder, setInputPlaceholder] = useState(`What's on your mind, ${userName}`);\n    const [body, setBody] = useState(\"\");\n    const [showDiv, setShowDiv] = useState(false);\n    \n\n    const handleInputClick = (e) => {\n        e.preventDefault();\n        setShowDiv(true)\n    }\n    \n    return (\n        <div className=\"createPostDiv\">\n            <section className=\"inputSectio